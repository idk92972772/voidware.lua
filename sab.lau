    KillAura = false,        -- Feature 10
    AntiRagdoll = false,     -- Feature 11
    AntiTrapSentry = false,  -- Feature 12
    TeleportToBases = false, -- Feature 13
    InstantPrompts = false,  -- Feature 14
    WebhookRares = false,    -- Feature 15
    AutoCollectCash = false, -- Feature 16
    InvisibleSteal = false,  -- Feature 17 (Desync)
    AntiKick = true,         -- Feature 18
    TweenMovement = true,    -- Feature 19 (Safe TP/Speed)
    AutoFarm = false         -- Feature 20
}

-- Feature 1: Full AC Bypass (ObserveTag Hook + Anti-Kick)
if _G.VoidWare.AntiCheatBypass then
    local hooked = false
    for _, v in pairs(getgc(true)) do
        if typeof(v) == "table" then
            local fn = rawget(v, "observeTag")
            if typeof(fn) == "function" and not hooked then
                hooked = true
                hookfunction(fn, newcclosure(function(_, _)
                    return {Disconnect = function() end, disconnect = function() end}
                end))
            end
        end
    end
    -- Anti-Kick Override
    hookfunction(LocalPlayer.Kick, function() end)
    print("AC Bypassed: ObserveTag Hooked | Anti-Kick Active")
end

-- GUI (Using Simple UI for Simplicity - Expandable)
local ScreenGui = Instance.new("ScreenGui", game.CoreGui)
ScreenGui.Name = "VoidWareHub"
local Frame = Instance.new("Frame", ScreenGui)
Frame.Size = UDim2.new(0, 300, 0, 400)
Frame.Position = UDim2.new(0.5, -150, 0.5, -200)
Frame.BackgroundColor3 = Color3.new(0.1, 0.1, 0.1)
local UIList = Instance.new("UIListLayout", Frame)
UIList.Padding = UDim.new(0, 5)

local function CreateToggle(name, toggleVar)
    local Toggle = Instance.new("TextButton", Frame)
    Toggle.Size = UDim2.new(1, 0, 0, 30)
    Toggle.Text = name .. ": OFF"
    Toggle.BackgroundColor3 = Color3.new(0.2, 0.2, 0.2)
    Toggle.MouseButton1Click:Connect(function()
        _G.VoidWare[toggleVar] = not _G.VoidWare[toggleVar]
        Toggle.Text = name .. ": " .. (_G.VoidWare[toggleVar] and "ON" or "OFF")
    end)
end

