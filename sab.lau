local LocalPlayer = Players.LocalPlayer
local Character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
local Humanoid = Character:WaitForChild("Humanoid")
local HRP = Character:WaitForChild("HumanoidRootPart")

-- Global Toggles
_G.VoidWare = {
    Enabled = true,
    AntiCheatBypass = true,  -- Feature 1
    AutoSteal = false,       -- Feature 2
    AutoLockBase = false,    -- Feature 3
    ESP = false,             -- Feature 4
    Fly = false,             -- Feature 5
    SpeedHack = false,       -- Feature 6
    NoClip = false,          -- Feature 7
    GodMode = false,         -- Feature 8
